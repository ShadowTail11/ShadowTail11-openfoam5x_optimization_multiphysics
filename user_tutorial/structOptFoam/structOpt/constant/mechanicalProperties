FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Density
rho
{
    type        uniform;
    value       1;
}

// Poisson's Ratio
Po
{
    type        uniform;
    value       0.3;
}

// Young's Modulus
E
{
    type        uniform;
    value        2e+11;
}

planeStress     yes;


